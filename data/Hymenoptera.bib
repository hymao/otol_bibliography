}

@article{liu_new_2012,
	title = {A {New} {Species} of {Saphonecrus} ({Hymenoptera}, {Cynipoidea}) {Associated} {With} {Plant} {Galls} on {Castanopsis} ({Fagaceae}) in {China}},
